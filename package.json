    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/testing": "10.2.10",
    "@types/jest": "29.5.11",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/node": "20.10.3",
